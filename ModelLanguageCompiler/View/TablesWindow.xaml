<Window x:Class="ModelLanguageCompiler.View.TablesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ModelLanguageCompiler.View"
        mc:Ignorable="d"
    Title="Таблицы" Height="450" Width="800">
    <Grid Margin="10">
        <TabControl>
            <TabItem Header="Служебные слова">
                <ListBox Name="ListWords"/>
            </TabItem>
            <TabItem Header="Ограничители">
                <ListBox Name="ListLimiters"/>
            </TabItem>
            <TabItem Header="Числа">
                <ListBox Name="ListNumbers"/>
            </TabItem>
            <TabItem Header="Идентификаторы">
                <ListBox Name="ListIds"/>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
